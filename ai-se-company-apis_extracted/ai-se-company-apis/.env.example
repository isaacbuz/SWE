# .env.example
# Copy to .env and fill values. Use direnv or dotenv for local dev.

# Google OAuth / Service Account (choose one path)
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:5173/callback

# OR Service Account (JSON contents or path)
GOOGLE_SERVICE_ACCOUNT_JSON_PATH=./service_account.json

# U.S. GSA / api.data.gov
GSA_API_KEY=

# GitHub (optional but recommended to increase rate limits)
GITHUB_TOKEN=
