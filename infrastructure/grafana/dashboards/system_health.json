{
  "dashboard": {
    "title": "SWE Platform - System Health",
    "tags": ["swe-platform", "system", "health"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Requests/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Latency (p95, p99)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 {{endpoint}}",
            "refId": "B"
          }
        ],
        "yaxes": [{ "format": "s", "label": "Duration" }, { "format": "short" }]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "5xx errors - {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{status=~\"4..\"}[5m])",
            "legendFormat": "4xx errors - {{endpoint}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Errors/sec" },
          { "format": "short" }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [10], "type": "gt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "5m", "now"] },
              "reducer": { "params": [], "type": "avg" },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "System CPU Usage",
        "type": "gauge",
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": [
            { "color": "green", "value": 0 },
            { "color": "yellow", "value": 70 },
            { "color": "red", "value": 90 }
          ]
        }
      },
      {
        "id": 5,
        "title": "System Memory Usage",
        "type": "gauge",
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
        "targets": [
          {
            "expr": "system_memory_usage_percent",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": [
            { "color": "green", "value": 0 },
            { "color": "yellow", "value": 80 },
            { "color": "red", "value": 95 }
          ]
        }
      },
      {
        "id": 6,
        "title": "Database Query Performance",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 {{operation}} on {{table}}",
            "refId": "A"
          }
        ],
        "yaxes": [{ "format": "s", "label": "Duration" }, { "format": "short" }]
      },
      {
        "id": 7,
        "title": "Database Connection Pool",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "db_active_connections",
            "legendFormat": "Active - {{pool_name}}",
            "refId": "A"
          },
          {
            "expr": "db_connection_pool_size",
            "legendFormat": "Pool Size - {{pool_name}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Connections" },
          { "format": "short" }
        ]
      },
      {
        "id": 8,
        "title": "Cache Hit Ratio",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "targets": [
          {
            "expr": "cache_hit_ratio",
            "legendFormat": "{{cache_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percentunit", "label": "Hit Ratio", "max": 1, "min": 0 },
          { "format": "short" }
        ]
      },
      {
        "id": 9,
        "title": "Active Agents",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "targets": [
          {
            "expr": "active_agents",
            "legendFormat": "{{agent_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          }
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}
