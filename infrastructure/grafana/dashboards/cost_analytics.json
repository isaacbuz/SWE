{
  "dashboard": {
    "title": "SWE Platform - Cost Analytics",
    "tags": ["swe-platform", "costs", "analytics"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Total LLM Costs (24h)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(llm_cost_usd_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "text": {
            "valueSize": 38
          },
          "unit": "currencyUSD"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Cost Rate (USD/hour)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "rate(llm_cost_usd_total[1h]) * 3600",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "text": {
            "valueSize": 38
          },
          "unit": "currencyUSD"
        }
      },
      {
        "id": 3,
        "title": "Average Cost per Request",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "avg(cost_per_request_usd)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "unit": "currencyUSD"
        }
      },
      {
        "id": 4,
        "title": "Budget Utilization",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "avg(budget_utilization_percent)",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": [
            {"color": "green", "value": 0},
            {"color": "yellow", "value": 70},
            {"color": "orange", "value": 85},
            {"color": "red", "value": 95}
          ],
          "unit": "percent"
        }
      },
      {
        "id": 5,
        "title": "Cost Trend (7 days)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "sum(rate(llm_cost_usd_total[1h])) * 3600",
            "legendFormat": "Hourly Cost",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Cost (USD)"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Cost by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "sum by (provider) (rate(llm_cost_usd_total[1h])) * 3600",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Cost (USD/hour)"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Cost by Model",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "targets": [
          {
            "expr": "sum by (model) (increase(llm_cost_usd_total[24h]))",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Cost per Project",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "project_cost_usd",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "project_id": "Project ID",
                "Value": "Cost (USD)"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "Cost per Request Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "cost_per_request_usd",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": true,
          "calculation": {},
          "cellGap": 2,
          "cellRadius": 0,
          "cellValues": {},
          "color": {
            "mode": "scheme",
            "scheme": "Spectral",
            "steps": 128
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "currencyUSD"
          }
        }
      },
      {
        "id": 10,
        "title": "Token Usage by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "sum by (provider) (rate(llm_tokens_total[5m]))",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Tokens/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Cost Efficiency (Cost per 1K tokens)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "targets": [
          {
            "expr": "(rate(llm_cost_usd_total[5m]) / rate(llm_tokens_total[5m])) * 1000",
            "legendFormat": "{{provider}}/{{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Cost per 1K tokens"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "ROI by Project",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 38},
        "targets": [
          {
            "expr": "roi_percent",
            "legendFormat": "{{project_id}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "showUnfilled": true,
          "text": {},
          "unit": "percent"
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
