{
  "dashboard": {
    "title": "SWE Platform - Quality Metrics",
    "tags": ["swe-platform", "quality", "testing"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Quality Gate Success Rate",
        "type": "stat",
        "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "sum(rate(quality_gates_total{status=\"passed\"}[24h])) / sum(rate(quality_gates_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "unit": "percentunit",
          "text": {
            "valueSize": 38
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 0.8 },
                { "color": "green", "value": 0.95 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Average Code Coverage",
        "type": "gauge",
        "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
        "targets": [
          {
            "expr": "avg(code_coverage_percent)",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": [
            { "color": "red", "value": 0 },
            { "color": "yellow", "value": 70 },
            { "color": "green", "value": 80 }
          ],
          "unit": "percent"
        }
      },
      {
        "id": 3,
        "title": "Average Code Quality Score",
        "type": "gauge",
        "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "avg(code_quality_score)",
            "refId": "A"
          }
        ],
        "options": {
          "thresholds": [
            { "color": "red", "value": 0 },
            { "color": "yellow", "value": 70 },
            { "color": "green", "value": 85 }
          ],
          "unit": "none",
          "min": 0,
          "max": 100
        }
      },
      {
        "id": 4,
        "title": "Critical Vulnerabilities",
        "type": "stat",
        "gridPos": { "h": 6, "w": 6, "x": 18, "y": 0 },
        "targets": [
          {
            "expr": "sum(security_vulnerabilities{severity=\"critical\"})",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "text": {
            "valueSize": 38
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 5 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Quality Gate Executions",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
        "targets": [
          {
            "expr": "rate(quality_gates_total{status=\"passed\"}[5m])",
            "legendFormat": "Passed - {{gate_type}}",
            "refId": "A"
          },
          {
            "expr": "rate(quality_gates_total{status=\"failed\"}[5m])",
            "legendFormat": "Failed - {{gate_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "ops", "label": "Executions/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 6,
        "title": "Quality Gate Duration",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(quality_gate_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{gate_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [{ "format": "s", "label": "Duration" }, { "format": "short" }]
      },
      {
        "id": 7,
        "title": "Code Coverage by Project",
        "type": "bargauge",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
        "targets": [
          {
            "expr": "code_coverage_percent",
            "legendFormat": "{{project_id}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          },
          "showUnfilled": true,
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 70 },
                { "color": "green", "value": 80 }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Security Vulnerabilities by Severity",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
        "targets": [
          {
            "expr": "sum by (severity) (security_vulnerabilities)",
            "legendFormat": "{{severity}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Quality Gate Pass/Fail Ratio",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
        "targets": [
          {
            "expr": "sum by (gate_type) (rate(quality_gates_total{status=\"passed\"}[5m])) / sum by (gate_type) (rate(quality_gates_total[5m]))",
            "legendFormat": "{{gate_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percentunit", "label": "Pass Rate", "max": 1, "min": 0 },
          { "format": "short" }
        ]
      },
      {
        "id": 10,
        "title": "Code Quality Score Trend",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
        "targets": [
          {
            "expr": "code_quality_score",
            "legendFormat": "{{project_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "none", "label": "Quality Score", "max": 100, "min": 0 },
          { "format": "short" }
        ]
      },
      {
        "id": 11,
        "title": "Test Success Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 30 },
        "targets": [
          {
            "expr": "rate(tasks_completed_total{task_type=\"test\"}[5m]) / (rate(tasks_completed_total{task_type=\"test\"}[5m]) + rate(tasks_failed_total{task_type=\"test\"}[5m]))",
            "legendFormat": "{{project_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Success Rate",
            "max": 1,
            "min": 0
          },
          { "format": "short" }
        ]
      },
      {
        "id": 12,
        "title": "User Satisfaction Score",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 30 },
        "targets": [
          {
            "expr": "avg(user_satisfaction_score)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "unit": "none",
          "text": {
            "valueSize": 60
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 3 },
                { "color": "green", "value": 4 }
              ]
            },
            "min": 1,
            "max": 5,
            "decimals": 2
          }
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
