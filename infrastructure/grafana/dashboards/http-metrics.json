{
  "dashboard": {
    "title": "SWE Platform - HTTP Metrics",
    "tags": ["swe-platform", "http", "api"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate by Route",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(swe_platform_http_requests_total[5m])) by (route)",
            "legendFormat": "{{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Request Duration Percentiles",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(swe_platform_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(swe_platform_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(swe_platform_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Error Rate by Status Code",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(swe_platform_http_request_errors_total[5m])) by (status_code)",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Requests by Method",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(swe_platform_http_requests_total[5m])) by (method)",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Error Rate by Route",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "topk(10, sum(rate(swe_platform_http_request_errors_total[5m])) by (route))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "route": "Route",
                "Value": "Error Rate"
              }
            }
          }
        ]
      },
      {
        "id": 6,
        "title": "Request Duration Heatmap",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(swe_platform_http_request_duration_seconds_bucket[5m])) by (le, route)",
            "format": "heatmap",
            "legendFormat": "{{route}}",
            "refId": "A"
          }
        ],
        "yaxes": {
          "format": "s",
          "label": "Duration"
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}

